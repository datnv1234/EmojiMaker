<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt">
    <item android:left="1.5dp" android:top="1.5dp" android:right="0dp" android:bottom="0dp">
        <shape android:shape="rectangle">
            <solid android:color="@color/black"/>
            <corners android:radius="999dp"/>
        </shape>
    </item>
    <item android:left="0dp" android:top="0dp" android:right="1dp" android:bottom="1dp">
        <shape android:shape="rectangle">
            <solid android:color="@color/white"/>
            <corners android:radius="999dp"/>
            <stroke android:width="0.5dp" android:color="@color/black"/>
        </shape>
    </item>
</layer-list>
