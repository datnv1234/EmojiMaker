<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="RippleCornerTextView">
        <attr name="cornerRadius" format="dimension" />
        <attr name="startGradientColor" format="string" />
        <attr name="endGradientColor" format="string" />
        <attr name="strokeWidthT" format="float" />
        <attr name="strokeColorT" format="string" />
        <attr name="backgroundColorView" format="string" />
    </declare-styleable>

    <declare-styleable name="StickerView">
        <attr name="showIcons" format="boolean" />
        <attr name="showBorder" format="boolean" />
        <attr name="bringToFrontCurrentSticker" format="boolean" />
        <attr name="borderColor" format="color" />
        <attr name="borderAlpha" format="integer" />
        <attr name="showAuxiliaryLines" format="boolean" />
        <attr name="accentColor" format="color" />
        <attr name="dashColor" format="color" />
        <attr name="iconColor" format="color" />
    </declare-styleable>

    <declare-styleable name="FullscreenAttrs">
        <attr name="fullscreenBackgroundColor" format="color" />
        <attr name="fullscreenTextColor" format="color" />
    </declare-styleable>
</resources>